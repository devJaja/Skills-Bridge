{
  "name": "@doma-protocol/orderbook-sdk",
  "version": "0.1.4",
  "description": "Doma Orderbook SDK",
  "license": "MIT",
  "author": "Doma Protocol",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "generate-types": "typechain --target=ethers-v6 'abi/*.json' --out-dir './src/contracts'",
    "codegen": "barrelsby --config barrelsby.json",
    "lint": "eslint src/",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@opensea/seaport-js": "^4.0.4",
    "axios": "^1.9.0",
    "ethers": "^6.14.1"
  },
  "peerDependencies": {
    "viem": "^2.33.3"
  },
  "devDependencies": {
    "@typechain/ethers-v6": "^0.5.1",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.14.9",
    "barrelsby": "^2.3.3",
    "eslint": "^9.22.0",
    "tslib": "^2.8.1",
    "typechain": "^8.3.2",
    "typescript": "^5.8.3"
  }
}
